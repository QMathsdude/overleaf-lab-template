% ---------------------
% Sample bibilography

1. Journal Article
@article{knuth1984,
    author = {Donald E. Knuth},
    title = {Literate Programming},
    journal = {The Computer Journal},
    volume = {27},
    number = {2},
    pages = {97--111},
    year = {1984},
    publisher = {Oxford University Press}
}

2. Book
@book{latexcompanion,
    author = {Frank Mittelbach and Michel Goossens},
    title = {The \LaTeX\ Companion},
    year = {2004},
    publisher = {Addison-Wesley},
    edition = {2nd}
}

3. Manual (technical manuals or documentation)
@manual{latexguide,
    title = {LaTeX User's Guide and Reference Manual},
    author = {Leslie Lamport},
    organization = {Addison-Wesley Publishing Company},
    year = {1994}
}

4. Website
@misc{overleaf,
    author = {{Overleaf Team}},
    title = {Overleaf Documentation on BibTeX},
    howpublished = {\url{https://www.overleaf.com/learn/latex/Bibliography_management_with_bibtex}},
    note = {Accessed: 2025-03-06}
}

-----------------------------
5. Inbook (citing a specific chapter or section of a book)
@inbook{knuth1997,
    author = {Donald E. Knuth},
    title = {The Art of Computer Programming},
    chapter = {Sorting and Searching},
    publisher = {Addison-Wesley},
    year = {1997},
    pages = {321--400}
}

6. InCollection (citing a part of a book that has multiple authors, e.g., an edited collection)
@incollection{turing1948,
    author = {Alan Turing},
    title = {Intelligent Machinery},
    booktitle = {Collected Works of A.M. Turing},
    editor = {B. Jack Copeland},
    publisher = {Oxford University Press},
    year = {1948},
    pages = {87--105}
}

7. Unpublished (not formally published but available)
@unpublished{newton1687,
   author={Isaac Newton}, 
   title={Principia Mathematica Draft}, 
   note={Unpublished manuscript}, 
   year={1687} 
}

8. Conference Proceedings (for entire conference)
@proceedings{conference2025,
   editor={John Smith and Jane Doe}, 
   title={Proceedings of the International Conference on Physics}, 
   year={2025}, 
   address={Berlin, Germany}, 
   publisher={Springer} 
}


9. Conference InProceedings (papers presented at conferences)
@inproceedings{watson1953structure,
  title={The structure of DNA},
  author={Watson, James D and Crick, Francis HC},
  booktitle={Cold Spring Harbor symposia on quantitative biology},
  volume={18},
  pages={123--131},
  year={1953},
  organization={Cold Spring Harbor Laboratory Press}
}

10. Masters Thesis
@mastersthesis{smith2020,
    author = {John Smith},
    title = {Advanced Topics in Quantum Mechanics},
    school = {University of Physics},
    year = {2020}
}

11. Phd. Thesis
@phdthesis{smith2020,
    author = {John Smith},
    title = {Advanced Topics in Quantum Mechanics},
    school = {University of Physics},
    year = {2020}
}

12. Tech Report (technical reports published by institutions)
@techreport{nasa2024,
    author = {NASA Research Team},
    title = {Mars Exploration Program: 2024 Update Report},
    institution = {NASA Jet Propulsion Laboratory},
    year = {2024}
}